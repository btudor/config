			<!ELEMENT working_hours (#PCDATA)>
			<!ELEMENT job_title (#PCDATA)>
			<!ELEMENT advertiser_name (#PCDATA)>
			<!ELEMENT location_job (#PCDATA)>
			<!ELEMENT salary_text (#PCDATA)>
			<!ELEMENT summary_job_description (#PCDATA)>
			<!ELEMENT orderno (#PCDATA)>
			<!ELEMENT industry_sector (#PCDATA)>